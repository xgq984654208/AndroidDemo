<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepView">
        <!--背景-->
        <attr name="backGroundColor" format="color"/>
        <!--最外层圆-->
        <attr name="outerCircleColor" format="color"/>
        <!--外层圆上的小圆点颜色-->
        <attr name="outerDotColor" format="color"/>
        <!--竖线的颜色-->
        <attr name="lineColor" format="color"/>
        <!--圆环的颜色-->
        <attr name="ringColor" format="color"/>
        <!--步数颜色-->
        <attr name="stepNumColor" format="color"/>
        <!--其他字的颜色-->
        <attr name="othetTextColor" format="color"/>

    </declare-styleable>
</resources>